<!doctype html>
<html class="no-js" lang="">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title><%= config.title %></title>
    <meta name="description" content="Uneak Products a division of BCI designs, manufactures and markets high quality Christmas tree stands and accessories throughout the U.S. and Canada.">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="Copyright" content="2017 Uneak Products, www.uneakproducts.com">
    <meta name="Language" content="en">
    <meta name="Distribution" content="Global">
    <meta name="Robots" content="index, follow">
    <meta name="Rating" content="General">
    <meta property="og:locale" content="en_US">
    <meta property="og:type" content="website">
    <meta property="og:title" content="<%= config.title %>">
    <meta property="og:description" content="Uneak Products a division of BCI designs, manufactures and markets high quality Christmas tree stands and accessories throughout the U.S. and Canada.">
    <meta property="og:url" content="https://uneakproducts.com/">
    <meta property="og:site_name" content="Uneak Products">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:description" content="Uneak Products a division of BCI designs, manufactures and markets high quality Christmas tree stands and accessories throughout the U.S. and Canada.">
    <meta name="twitter:title" content="<%= config.title %>">
    <link rel="stylesheet" href="./styles.css">
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
  </head>
  <body>
    <div id="container"></div>
    <script src="<%= bundle %>"></script>
    <%_ if (!debug && config.trackingID) { _%>
    <script>
      window.ga=function(){ga.q.push(arguments)};ga.q=[];ga.l=+new Date;
      ga('create','<%= config.trackingID %>','auto');ga('send','pageview')
    </script>
    <script src="https://www.google-analytics.com/analytics.js" async defer></script>
    <%_ } _%>
  </body>
</html>
